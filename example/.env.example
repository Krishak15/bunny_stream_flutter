# Bunny Stream Example Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env to version control

# Your Bunny Stream Library ID (numeric)
BUNNY_LIBRARY_ID=12345

# Your Bunny Stream Access Key
BUNNY_ACCESS_KEY=your-stream-access-key-here

# Video ID to play (from your Bunny library)
BUNNY_VIDEO_ID=video-guid-here

# Collection ID
BUNNY_COLLECTION_ID=collection-guid-here

# Optional: Custom CDN hostname (leave empty to use default)
BUNNY_CDN_HOSTNAME=

# Optional: Secure token for playback (from your backend)
BUNNY_SECURE_TOKEN=

# Optional: Token expiry unix timestamp
BUNNY_EXPIRES=
